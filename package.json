{
  "name": "breathbox",
  "version": "1.0.0",
  "description": "Browser Breathbox (npm for testing only)",
  "main": "null",
  "scripts": {
    "build-dev": "webpack --config ./webpack/webpack-build.config.js --mode development",
    "build-prod": "webpack --config ./webpack/webpack-build.config.js --mode production",
    "components": "webpack --config ./webpack/webpack-components.config.js",
    "htmlgen": "npm run build && webpack --config ./webpack/webpack-components.config.js && webpack --config ./webpack/webpack-index.config.js",
    "htmlgen-prod": "npm run build && webpack --config ./webpack/webpack-components.config.js && webpack --config ./webpack/webpack-index.config.js --mode production",
    "test": "npm run lint && jest",
    "format": "prettier --write '**/*.{js,ts,tsx,json}' && eslint babel.config.js src/{ts,components}/* --fix",
    "lint": "eslint src/*"
  },
  "author": "michael budnick",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.23.2",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-react-constant-elements": "^7.22.5",
    "@babel/plugin-transform-react-inline-elements": "^7.22.5",
    "@babel/plugin-transform-runtime": "^7.23.2",
    "@babel/plugin-transform-typescript": "^7.22.15",
    "@babel/preset-env": "^7.23.2",
    "@babel/preset-react": "^7.22.15",
    "@babel/preset-typescript": "^7.23.2",
    "@babel/runtime": "^7.23.2",
    "@types/jest": "^29.5.5",
    "@types/react": "^18.2.33",
    "@types/react-dom": "^18.2.14",
    "@typescript-eslint/eslint-plugin": "^6.8.0",
    "babel-loader": "^9.1.3",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "css-loader": "^6.8.1",
    "eslint": "^8.51.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-standard-with-typescript": "^39.1.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-n": "^16.2.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-promise": "^6.1.1",
    "html-webpack-plugin": "^5.5.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jsx-loader": "^0.9.0",
    "mini-css-extract-plugin": "^2.7.6",
    "prettier": "^3.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router": "^6.17.0",
    "static-site-generator-webpack-plugin": "^3.3.0",
    "style-loader": "^3.3.3",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "webpack-node-externals": "^3.0.0"
  }
}
