{
  "name": "breathbox",
  "version": "1.0.0",
  "description": "Browser Breathbox (npm for testing only)",
  "main": "null",
  "scripts": {
    "build": "webpack --config ./webpack/webpack-ts.config.js && npm run concat-css",
    "components": "webpack --config ./webpack/webpack-components.config.js",
    "htmlgen": "npm run build && webpack --config ./webpack/webpack-components.config.js && webpack --config ./webpack/webpack-index.config.js",
    "test": "npm run lint && jest",
    "format": "prettier --write 'src/**/*.{js,tsx,json}' && eslint src/{ts,components}/* --fix",
    "lint": "eslint src/*",
    "concat-css": "grep -h '' src/css/* > www/assets/css/style.css"
  },
  "author": "michael budnick",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^29.5.5",
    "@types/react": "^18.2.33",
    "@types/react-dom": "^18.2.14",
    "@typescript-eslint/eslint-plugin": "^6.8.0",
    "eslint": "^8.51.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-standard-with-typescript": "^39.1.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-n": "^16.2.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-promise": "^6.1.1",
    "html-webpack-plugin": "^5.5.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jsx-loader": "^0.9.0",
    "prettier": "^3.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router": "^6.17.0",
    "static-site-generator-webpack-plugin": "^3.3.0",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.5.0",
    "typescript": "^5.2.2",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "webpack-node-externals": "^3.0.0"
  }
}
